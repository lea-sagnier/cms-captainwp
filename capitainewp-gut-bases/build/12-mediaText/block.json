{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "capitainewp/media-text",
  "version": "0.1.0",
  "title": "12-mediaText",
  "category": "image",
  "icon": "edit",
  "description": "Editable img",
  "supports": {
    "html": false
  },
  "attributes": {
    "pictureID": {
      "type": "number",
      "default": null
    },
    "pictureURL": {
      "type": "string",
      "source": "attribute",
      "attribute": "src",
      "selector": "img"
    },
    "pictureAlt": {
      "type": "string",
      "source": "attribute",
      "attribute": "alt",
      "selector": "img"
    },
    "content": {
      "type": "string",
      "source": "text",
      "selector": ".content"
    }
  },
  "textdomain": "capitainewp-gut-bases",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}